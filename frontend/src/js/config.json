                },
                "SHOW_REMAINING_SUBMISSIONS": {
                    "JOBS_ENDPOINT": "jobs/",
                    "PHASES_ENDPOINT": "/phases/",
                    "REMAINING_SUBMISSIONS_ENDPOINT": "/remaining_submissions/"
                },
                "DOWNLOAD_CHALLENGE_SUBMISSIONS": {
                    "DOWNLOAD_ALL_SUBMISSIONS_ENDPOINT": "/download_all_submissions_file/"
                }
            }
