{
    "env": {
        "local": {
            "EnvironmentConfig": {
                "API": "http://localhost:8000/api/"
            }
        },
        "staging": {
            "EnvironmentConfig": {
                "API": "http://api.evalai.cloudcv.org/api/"
            }
        },
        "production": {
            "EnvironmentConfig": {
                "API": "https://evalapi.cloudcv.org/api/"
            }
        }
    },
    "urls": {
        "BackendEndpoints": {
            "AUTH": {
                "REGISTRATION_ENDPOINT": "auth/registration/",
                "LOGIN_ENDPOINT": "auth/login/",
                "EMAIL_VERIFICATION_ENDPOINT": "auth/registration/account-confirm-email/",
                "PASSWORD_RESET_ENDPOINT": "auth/password/reset/",
                "PASSWORD_RESET_CONFIRM_ENDPOINT": "auth/password/reset/confirm/"
            },
            "CHALLENGE": {
                "CREATE_CHALLENGE_ENDPOINT": "challenges/challenge/challenge_host_team/",
                "CHALLENGE_DETAILS_ENDPOINT": "challenges/challenge/",
                "PARTICIPANT_CHALLENGES_ENDPOINT": "participants/participant_teams/challenges/",
