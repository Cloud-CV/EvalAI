[run]
omit =
    */python?.?/*
    */migrations/*
source = evalai/

[report]
exclude_lines =
    pragma: no cover
    import
    @api_view
    @authentication_classes
    @permission_classes
    @csrf_exempt

omit =
    */templates/*
    */static/*
    */python?.?/*
    */migrations/*
    */tests/*
    */middleware/*
    frontend/
    settings/prod.py
    scripts/seed.py
